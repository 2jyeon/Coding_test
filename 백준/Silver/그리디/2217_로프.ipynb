    "n = int(input())\n",
    "\n",
    "ropes = []\n",
    "for _ in range(n):\n",
    "    ropes.append(int(input()))\n",
    "ropes.sort()\n",
    "\n",
    "answer = []\n",
    "for i in ropes:\n",
    "    answer.append(i * n)\n",
    "    n -= 1\n",
    "\n",
    "print(max(answer))"
   ]

    "def solution(n):\n",
    "    # 3진법 구하기\n",
    "    tmp = ''\n",
    "    tmp += str(n%3)\n",
    "    while n >= 3:\n",
    "        n = n // 3\n",
    "        tmp += str(n%3)\n",
    "        \n",
    "    # 10진법 구하기\n",
    "    answer = 0\n",
    "    for i in range(len(tmp)):\n",
    "        answer += int(tmp[len(tmp)-1-i]) * (3 ** i)\n",
    "        \n",
    "    return answer\n",
    "solution(3)"
   ]
  }
